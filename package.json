{
  "name": "vue3-ssr",
  "version": "1.0.0",
  "description": "基于vue3的服务端渲染。",
  "scripts": {
    "dev": "nodemon dev.server.mjs",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --ssrManifest --outDir ssr/client",
    "build:server": "vite build --ssr src/entry-server.js --outDir ssr/server",
    "server": "cross-env node prod.server.js",
    "debug": "nodemon --inspect-brk dev.server.mjs"
  },
  "keywords": ["vue3","ssr","vue3-ssr","vue-router","pinia","axios"],
  "author": "将梦拉到现实",
  "license": "ISC",
  "dependencies": {
    "@vue/server-renderer": "^3.2.37",
    "axios": "^0.27.2",
    "pinia": "^2.0.14",
    "vue": "^3.2.37",
    "vue-router": "^4.0.16"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.3.3",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "cross-env": "^7.0.3",
    "express": "^4.18.1",
    "less": "^4.1.3",
    "less-loader": "^11.0.0",
    "nodemon": "^2.0.16",
    "vite": "^2.9.12"
  }
}
